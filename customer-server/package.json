{
	"name": "customer-server",
	"version": "1.0.0",
	"description": "Customer Server Side Application that provides REST API to perform CRUD operations on customer data ",
	"scripts": {
		"start": "npm run build && npm run serve",
		"build": "npm run build-ts",
		"serve": "node dist/server.js",
		"watch-node": "nodemon dist/server.js",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"debug": "npm run build && npm run serve-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"test": "mocha -r ts-node/register src/**/*.spec.ts"
	},
	"author": "Harun Ozceyhan",
	"license": "ISC",
	"dependencies": {
		"@types/cookie-parser": "~1.4.2",
		"@types/express": "4.17.2",
		"@types/morgan": "~1.7.37",
		"@types/uuid": "^3.4.6",
		"cookie-parser": "^1.4.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"knex": "^0.20.8",
		"morgan": "^1.9.1",
		"nodemon": "^2.0.2",
		"objection": "^2.1.2",
		"pg": "^7.17.1",
		"typescript": "^3.7.5",
		"uuid": "^3.4.0"
	},
	"devDependencies": {
		"@babel/cli": "7.8.3",
		"@babel/core": "7.8.3",
		"@babel/node": "7.8.3",
		"@babel/preset-env": "7.8.3",
		"@types/chai": "^4.2.8",
		"@types/mocha": "^7.0.1",
		"@types/supertest": "^2.0.8",
		"@typescript-eslint/eslint-plugin": "^2.3.1",
		"@typescript-eslint/parser": "^2.3.1",
		"babel-eslint": "10.0.3",
		"chai": "^4.2.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb-typescript": "6.3.1",
		"mocha": "^10.2.0",
		"supertest": "^4.0.2",
		"ts-node": "^8.6.2",
		"ts-node-dev": "1.0.0-pre.44"
	}
